<TabControl x:Class="Explorip.Explorer.Controls.TabExplorerBrowser"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
            xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
            xmlns:local="clr-namespace:Explorip.Explorer.Controls"
            mc:Ignorable="d" AllowDrop="True" Background="Transparent"
            d:DesignHeight="450" d:DesignWidth="800" BorderThickness="0" Padding="0"
            Name="MyTabControl" IsTabStop="False" SelectionChanged="MyTabControl_SelectionChanged"
            SizeChanged="TabControl_SizeChanged">
