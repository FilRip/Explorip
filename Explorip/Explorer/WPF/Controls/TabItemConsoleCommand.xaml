<local:TabItemExplorip x:Class="Explorip.Explorer.WPF.Controls.TabItemConsoleCommand"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Explorip.Explorer.WPF.Controls"
             xmlns:viewmodels="clr-namespace:Explorip.Explorer.WPF.ViewModels"
             xmlns:consoleControl="clr-namespace:ConsoleControl.WPF;assembly=ConsoleControl.WPF"
             mc:Ignorable="d" Style="{DynamicResource CustomTabItem}"
             d:DesignHeight="450" d:DesignWidth="800">
    <TabItem.DataContext>
        <viewmodels:TabItemConsoleCommandViewModel/>
    </TabItem.DataContext>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <consoleControl:ConsoleControl 
            x:Name="MyConsoleControl"
            FontSize="14" Focusable="True"/>
    </Grid>
</local:TabItemExplorip>
